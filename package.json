{
  "name": "scouting-lommel-website",
  "private": true,
  "scripts": {
    "build:css:components": "postcss src/components/**/*.pcss --base src/components --dir src/components --ext css --env production",
    "build:css:global": "postcss src/assets/styles/global.pcss --output src/app/global.css --env production",
    "build:icons": "npm run generate-icons",
    "build:next": "next build",
    "build:prebuild": "node prebuild.ts",
    "build": "run-p build:**",
    "dev:css:components": "postcss src/components/**/*.pcss --base src/components --dir src/components --ext css -w",
    "dev:css:global": "postcss src/assets/styles/global.pcss --output src/app/global.css -w",
    "dev:icons": "npm run generate-icons",
    "dev:next": "next dev",
    "dev": "run-p dev:**",
    "export": "next export",
    "generate-icons": "node ./script/generate-icons.js && npx eslint ./src/assets/icons/index.js --quiet --fix",
    "lint:eslint": "next lint",
    "lint:typescript": "tsc",
    "lint:stylelint": "npx stylelint \"**/*.pcss\" --fix",
    "lint": "run-p lint:**",
    "preinstall": "npx only-allow npm",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:start": "storybook dev -p 6006",
    "storybook": "run-p generate-icons dev:css:* storybook:start",
    "build-storybook": "run-p generate-icons build:css:* storybook:build"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@ckeditor/ckeditor5-build-classic": "^34.2.0",
    "@ckeditor/ckeditor5-react": "^5.0.3",
    "@svgr/webpack": "^6.5.1",
    "apollo-upload-client": "^17.0.0",
    "classnames": "^2.3.2",
    "graphql": "^16.6.0",
    "jwt-decode": "^3.1.2",
    "next": "^13.4.7",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5",
    "remark-gfm": "^3.0.1",
    "typescript": "^5.0.4",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^1.0.3",
    "@netlify/plugin-nextjs": "^4.37.4",
    "@storybook/addon-actions": "^7.0.21",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/blocks": "^7.0.21",
    "@storybook/nextjs": "^7.0.21",
    "@storybook/react": "^7.0.21",
    "@storybook/testing-library": "^0.1.0",
    "@types/react": "^18.2.6",
    "autoprefixer": "^10.4.13",
    "cssnano": "^6.0.1",
    "eslint": "8.19.0",
    "eslint-config-next": "^13.4.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "json5": "^1.0.2",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "postcss-custom-media": "^9.1.4",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.4.2",
    "prettier": "^2.4.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "storybook": "^7.0.21",
    "storybook-css-modules-preset": "^1.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0"
  }
}
