.typography {
  &--pre-wrap {
    white-space: pre-wrap;
  }

  &--styled {
    margin: 0;
    color: var(--color-text);
    font-family: var(--font-family-body);
    font-size: 1rem;

    @media (--mq-from-medium) {
      font-size: 1.25rem;
    }

    @media (--mq-from-large) {
      font-size: 1.375rem;
    }
  }
}
